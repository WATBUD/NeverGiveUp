function chineseNum(a){numArray="\u96f6,\u4e00,\u4e8c,\u4e09,\u56db,\u4e94,\u516d,\u4e03,\u516b,\u4e5d,\u5341".split(",");decimalArray="\u500b,\u5341,\u767e,\u5343,\u842c,\u5341,\u767e,\u5343,\u5104,\u5341,\u767e,\u5343,\u5146".split(",");tmpstr=new String;for(counti=0;counti<a.toString().length;counti++)decimalPlace=a.toString().length-counti-1,2==decimalPlace&&1!=a.toString().charAt(counti)&&2==a.toString().length||(tmpstr+=numArray[a.toString().charAt(counti)]),-1<decimalPlace&&(tmpstr+=decimalArray[decimalPlace],
tmpstr=tmpstr.replace("\u4e00\u5341\u5104","\u5341\u5104"),tmpstr=tmpstr.replace("\u4e00\u5341\u842c","\u5341\u842c"),tmpstr=tmpstr.replace("\u96f6\u842c","\u842c"),tmpstr=tmpstr.replace("\u96f6\u5104","\u5104"),tmpstr=tmpstr.replace("\u96f6\u5341","\u96f6"),tmpstr=tmpstr.replace("\u96f6\u767e","\u96f6"),tmpstr=tmpstr.replace("\u96f6\u5343","\u96f6"),tmpstr=tmpstr.replace("\u5341\u96f6","\u5341"),tmpstr=tmpstr.replace("\u767e\u96f6\u96f6","\u767e"),tmpstr=tmpstr.replace("\u5343\u96f6\u96f6\u96f6",
"\u5343"),tmpstr=tmpstr.replace("\u5343\u96f6\u96f6","\u5343\u96f6"),tmpstr=tmpstr.replace("\u842c\u96f6\u96f6\u96f6\u96f6","\u842c"),tmpstr=tmpstr.replace("\u842c\u96f6\u96f6\u96f6","\u842c\u96f6"),tmpstr=tmpstr.replace("\u842c\u96f6\u96f6","\u842c\u96f6"),tmpstr=tmpstr.replace("\u5104\u96f6\u96f6\u96f6\u96f6","\u5104"),tmpstr=tmpstr.replace("\u5104\u96f6\u96f6\u96f6","\u5104\u96f6"),tmpstr=tmpstr.replace("\u5104\u96f6\u96f6","\u5104\u96f6"),tmpstr=tmpstr.replace("\u5104\u842c","\u5104"),tmpstr=
tmpstr.replace("\u96f6\u500b",""),tmpstr=tmpstr.replace("\u500b",""));return tmpstr}function CountingPrice(){for(var a=document.getElementsByName("counting_price"),c=document.getElementsByName("material_num"),d=0,b=0;b<a.length;b++)d=parseInt(d)+parseInt(c[b].innerHTML)*parseInt(a[b].value);a=document.getElementById("total_price");c=document.getElementById("total_price_c");a.innerHTML=parseInt(d);c.innerHTML=chineseNum(parseInt(d))}
function CountingPrice1(){for(var a=document.getElementsByName("counting_price1"),c=document.getElementsByName("material_num1"),d=0,b=0;b<a.length;b++)""==a[b].value&&(a[b].value=0),d=parseInt(d)+parseInt(c[b].innerHTML)*parseInt(a[b].value);a=document.getElementById("total_price1");c=document.getElementById("total_price_c1");a.innerHTML=parseInt(d);c.innerHTML=chineseNum(parseInt(d))}
function CountingPrice2(){for(var a=document.getElementsByName("counting_price2"),c=document.getElementsByName("material_num2"),d=0,b=0;b<a.length;b++)d=parseInt(d)+parseInt(c[b].innerHTML)*parseInt(a[b].value);a=document.getElementById("total_price2");c=document.getElementById("total_price_c2");a.innerHTML=parseInt(d);c.innerHTML=chineseNum(parseInt(d))}
function CountingPriceN(a){for(var c=document.getElementsByName("counting_price"+a),d=document.getElementsByName("material_num"+a),b=0,e=0;e<c.length;e++)b=parseInt(b)+parseInt(d[e].innerHTML)*parseInt(c[e].value);c=document.getElementById("total_price"+a);a=document.getElementById("total_price_c"+a);c.innerHTML=parseInt(b);a.innerHTML=chineseNum(parseInt(b))};